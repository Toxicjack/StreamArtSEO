  "shortcuts": [
    {
      "name": "YouTube Dashboard",
      "short_name": "YouTube",
      "description": "View your YouTube analytics and content",
      "url": "/videos",
      "icons": [{ "src": "/images/icons/youtube-icon.png", "sizes": "192x192" }]
    },
    {
      "name": "DeviantArt Gallery",
      "short_name": "Art",
      "description": "Browse your DeviantArt gallery",
      "url": "/art",
      "icons": [{ "src": "/images/icons/deviantart-icon.png", "sizes": "192x192" }]
    },
    {
      "name": "Twitch Streams",
      "short_name": "Streams",
      "description": "Manage your Twitch streams",
      "url": "/streams",
      "icons": [{ "src": "/images/icons/twitch-icon.png", "sizes": "192x192" }]
    },
    {
      "name": "Reddit Posts",
      "short_name": "Reddit",
      "description": "View your Reddit posts and engagement",
      "url": "/posts",
      "icons": [{ "src": "/images/icons/reddit-icon.png", "sizes": "192x192" }]
    }
  ],
  "related_applications": [
    {
      "platform": "web"
    }
  ],
  "prefer_related_applications": false,
  "categories": ["productivity", "social", "utilities"],
  "lang": "en-US",
  "dir": "ltr",
  "scope": "/",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/csv": [".csv"]
      }
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "video/*"]
        }
      ]
    }
  }
}